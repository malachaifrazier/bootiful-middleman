{% set auth_pages = ['Signup', 'Login', 'ForgotPassword'] %}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" type="image/png" sizes="16x16" href="data:image/svg+xml;base64," />
    <link rel="stylesheet" href="/css/style.css" />
    {% block stylesheets %}{% endblock %}

    <title>Bootiful | Admin | {{ page_name }}</title>
    <meta name="keywords" content="{{ SEO_keywords }}">
    <meta name="description" content="{{ SEO_description }}">
    <meta name="og:title" content="Bootiful {{ page_name }}">
    <meta name="og:description" content="{{ SEO_description }}">
    <meta name="og:image" content="https://res.cloudinary.com/bdavidxyz-com/image/upload/v1637305459/bootstrap/Pages2.png">

    {% include "partials/analytics.html" %}

  </head>
  <body class="admin container-fluid bg-light">
    <div class="row flex-nowrap">
      {% include "partials/admin_sidebar.html" %}
      <div class="main col pt-5">
        {% block content %}{% endblock %}
      </div>
    </div>

    <script src="/js/all.js"></script>
    {% block scripts %}{% endblock %}
  </body>
</html>
